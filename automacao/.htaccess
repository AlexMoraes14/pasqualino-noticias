Options -Indexes

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteRule (^|/)\.env(?:\..*)?$ - [F,L,NC]
    RewriteRule (^|/)env-example$ - [F,L,NC]
    RewriteRule (^|/)logs(?:/|$) - [F,L,NC]
</IfModule>

<FilesMatch "^(?:\.env(?:\..*)?|env-example)$">
    <IfModule mod_authz_core.c>
        Require all denied
    </IfModule>
    <IfModule !mod_authz_core.c>
        Order allow,deny
        Deny from all
    </IfModule>
</FilesMatch>
